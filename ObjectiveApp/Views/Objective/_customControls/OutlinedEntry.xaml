<?xml version="1.0" encoding="utf-8" ?>
<ContentView 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="ObjectiveApp.Views.Objective._customControls.OutlinedEntry"
    xmlns:globalTemplates="clr-namespace:ObjectiveApp.Views.CustomControls">

    <globalTemplates:OutlinedRoundedBorder>
        <Entry
            x:Name="textEntry"
            Text="{Binding Source={x:Reference this},Path=Text}"/>
    </globalTemplates:OutlinedRoundedBorder>
</ContentView>
